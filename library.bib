@article{Achille2017,
abstract = {We review the problem of defining and inferring a "state" for a control system based on complex, high-dimensional, highly uncertain measurement streams such as videos. Such a state, or representation, should contain all and only the information needed for control, and discount nuisance variability in the data. It should also have finite complexity, ideally modulated depending on available resources. This representation is what we want to store in memory in lieu of the data, as it "separates" the control task from the measurement process. For the trivial case with no dynamics, a representation can be inferred by minimizing the Information Bottleneck Lagrangian in a function class realized by deep neural networks. The resulting representation has much higher dimension than the data, already in the millions, but it is smaller in the sense of information content, retaining only what is needed for the task. This process also yields representations that are invariant to nuisance factors and having maximally independent components. We extend these ideas to the dynamic case, where the representation is the posterior density of the task variable given the measurements up to the current time, which is in general much simpler than the prediction density maintained by the classical Bayesian filter. Again this can be finitely-parametrized using a deep neural network, and already some applications are beginning to emerge. No explicit assumption of Markovianity is needed; instead, complexity trades off approximation of an optimal representation, including the degree of Markovianity.},
annote = {- projection
- tangent space
-},
archivePrefix = {arXiv},
arxivId = {1711.03321},
author = {Achille, Alessandro and Soatto, Stefano},
doi = {10.1146/annurev-control-060117},
eprint = {1711.03321},
file = {:Q\:/COURSES/paper/sampling-based-methods-for-motion-planning-with-constraints.pdf:pdf},
issn = {2573-5144},
journal = {Annual Review of Control, Robotics, and Autonomous Systems},
keywords = {constraints,high-dimensional robotic systems,planning for,planning with constraints,robot motion planning,robotics,sampling-based planning},
pages = {(to appear)},
title = {{A Separation Principle for Control in the Age of Deep Learning}},
url = {http://www.annualreviews.org/journal/control http://arxiv.org/abs/1711.03321},
year = {2017}
}
@article{Yao2005,
abstract = {In this paper, we address the path planning problem with general end-effector\nconstraints (PPGEC) for robot manipulators. Two approaches are proposed.\nThe first approach is adapted from an existing randomized gradient\ndescent (RGD) method for closed-chain robots. The second approach\nis radically different. We call it ATACE alternate task-space and\nconfiguration-space exploration. Unlike the first approach which\nsearches purely in C-space, ATACE works in both task space and C-space.\nIt explores the task space for end-effector paths satisfying given\nconstraints, and utilizes trajectory tracking technique(s) as a local\nplanner(s) to track these paths in the configuration space. We have\nimplemented both approaches and compare their relative performances\nin different scenarios. ATACE outperforms RGD in majority (but not\nall) of the scenarios. We outline intuitive explanations for the\nrelative performances of these two approaches.},
author = {Yao, Zhenwang and Gupta, Kamal},
doi = {10.1109/IROS.2005.1545305},
file = {:Q\:/COURSES/paper/Yao, Gupta - 2005 - Path planning with general end-effector constraints Using task space to guide configuration space search.pdf:pdf},
isbn = {0780389123},
issn = {09218890},
journal = {2005 IEEE/RSJ International Conference on Intelligent Robots and Systems, IROS},
keywords = {ATACE,End-effector constraints,Path planning},
pages = {2211--2216},
title = {{Path planning with general end-effector constraints: Using task space to guide configuration space search}},
year = {2005}
}
