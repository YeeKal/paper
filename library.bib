doi = {10.1109/IROS.2017.8206470},
eprint = {1703.01661},
file = {:Q\:/COURSES/paper/SegICP Integrated deep semantic segmentation and pose estimation.pdf:pdf},
isbn = {9781538626825},
issn = {21530866},
journal = {IEEE International Conference on Intelligent Robots and Systems},
pages = {5784--5789},
title = {{SegICP: Integrated deep semantic segmentation and pose estimation}},
volume = {2017-Septe},
year = {2017}
}
@article{Yakey2001,
abstract = {We extend randomized path planning algorithms to the case of articulated\nrobots that have closed kinematic chains. This is an important class\nof problems, which includes applications such as manipulation planning\nusing multiple open-chain manipulators that cooperatively grasp an\nobject and planning for reconfigurable robots in which links might\nbe arranged in a loop to ease manipulation or locomotion. Applications\nalso exist in areas beyond robotics, including computer graphics,\ncomputational chemistry, and virtual prototyping. Such applications\ntypically involve high degrees of freedom and a parameterization\nof the configurations that satisfy closure constraints is usually\nnot available. We show how to implement key primitive operations\nof randomized path planners for general closed kinematics chains.\nThese primitives include the generation of random free configurations\nand the generation of local paths. To demonstrate the feasibility\nof our primitives for general chains, we show their application to\nrecently developed randomized planners and present computed results\nfor high-dimensional problems},
author = {Yakey, Jeffery H. and LaValle, Steven M. and Kavraki, Lydia E.},
doi = {10.1109/70.976030},
file = {:Q\:/COURSES/paper/Randomized Path Planning for Linkages With Closed.pdf:pdf},
issn = {1042296X},
journal = {IEEE Transactions on Robotics and Automation},
keywords = {Closed linkages,Kinematic chains,Randomized path planning,closed chains,constraint},
mendeley-tags = {closed chains,constraint},
title = {{Randomized path planning for linkages with closed kinematic chains}},
year = {2001}
}
